<struts>

    <package name="metric" namespace="/metric" extends="default">
    
        <action name="returnMetricPage" class="com.asiainfo.aiom.view.inventory.server.MetricListAction" method="returnMetricPage">
            <result>/WEB-INF/view/inventory/metric/metric.jsp</result>
        </action>
        
        <action name="listMetric" class="com.asiainfo.aiom.view.inventory.server.MetricListAction">
            <result type="json">
				<param name="property">resultMap</param>
			</result>
        </action>
        
    </package>
    
</struts>